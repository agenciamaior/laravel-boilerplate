{
    "name": "agenciamaior/laravel_boilerplate",
    "description": "Template padrão para início de projetos com Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Agência Maior",
            "email": "alexandre.sottani@agenciamaior.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "AgenciaMaior\\LaravelBoilerplate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenciaMaior\\LaravelBoilerplate\\LaravelBoilerplateServiceProvider"
            ]
        }
    },
    "require": {
        "laravel/framework": ">=5.5",
        "agenciamaior/bootstrap_forms": ">=0.0.1",
        "doctrine/dbal": ">=2.9"
    }
}
